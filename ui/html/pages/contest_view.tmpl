{{define "title"}}Contest #{{.Contest.ID}}{{end}}

{{define "main"}}
    {{with .Contest}}
    <div class='contest'>
        <div class='metadata'>
            <span>#{{.ID}}</span>
        </div>
        <div class='metadata'>
            <time>Contest Date {{.ContestDate}}</time>
            <time>Created: {{.Created}}</time>
        </div>
    </div>
    {{end}}
{{end}}